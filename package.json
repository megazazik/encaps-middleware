{
  "name": "encaps-middleware",
  "version": "0.0.1",
  "description": "Library to create middleware for independent modules created by encaps",
  "main": "dist/index.js",
  "module": "dist-es/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.es.json",
    "version": "npm run build && conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commit": "git-cz",
    "test": "ts-node node_modules/tape/bin/tape ./src/**/*.spec.ts"
  },
  "keywords": [
    "encaps",
    "redux",
    "middleware"
  ],
  "author": "megazazik <megazazik@rambler.ru>",
  "license": "MIT",
  "devDependencies": {
    "@types/tape": "^4.2.34",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "encaps": "^0.8.1",
    "redux": "^4.0.5",
    "tape": "^4.13.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "encaps": "^0.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/megazazik/react-context-refs.git"
  },
  "homepage": "https://github.com/megazazik/react-context-refs",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eventemitter3": "^4.0.0"
  },
  "files": [
    "dist",
    "dist-es",
    "CHANGELOG.md",
    "README.md"
  ]
}
